{
  "name": "hooks",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node build/server.js",
    "s": "run-p touch watch:build watch:compile",
    "build": "rm -rf build && webpack --mode production",
    "touch": "mkdir build 2>/dev/null; touch build/server.js",
    "watch:build": "nodemon --watch build --exec 'yarn start'",
    "watch:compile": "webpack --mode development --watch"
  },
  "dependencies": {
    "@types/express": "^4.17.0",
    "@types/node": "^12.0.10",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "awesome-typescript-loader": "^5.2.1",
    "express": "^4.17.1",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "source-map-loader": "^0.2.4",
    "typescript": "^3.5.2",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5",
    "webpack-node-externals": "^1.7.2"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  }
}
